                        <colgroup valign="top">
                           
                           <colgroup valign="top">
                              
                              <colgroup valign="top">
                                 
                                 <table class="dd">
                                    
                                    <tr>
                                       <td>0</td>
                                       <td>:</td>
                                       <td>bit type        </td>
                                    </tr>
                                    
                                    <tr>
                                       <td>1</td>
                                       <td>:</td>
                                       <td>byte type       </td>
                                    </tr>
                                    
                                    <tr>
                                       <td>2</td>
                                       <td>:</td>
                                       <td>word type       </td>
                                    </tr>
                                    
                                    <tr>
                                       <td>3</td>
                                       <td>:</td>
                                       <td>2 word type     </td>
                                    </tr>
                                    
                                    <tr>
                                       <td>4</td>
                                       <td>:</td>
                                       <td>real number type</td>
                                    </tr>
                                    </table>
                                 </colgroup>
                              </colgroup>
                           </colgroup>
                        </table>
                     
