                              <td>Overheat alarm(only Series 15)</td>  
                              <td>(OH)</td> </tr>
                           
                           <tr>
                              <td>#03</td> 
                              <td>:</td> 
                              <td>Sub-CPU error(only Series 15)</td>   
                              <td>(SB)</td> </tr>
                           
                           <tr>
                              <td>#04</td> 
                              <td>:</td> 
                              <td>Syncronized error</td>                
                              <td>(SN)</td> </tr>
                           
                           <tr>
                              <td>#05</td> 
                              <td>:</td> 
                              <td>Parameter switch on         </td>     
                              <td>(SW)</td> </tr>
                           
                           <tr>
                              <td>#06</td> 
                              <td>:</td> 
                              <td>Overtravel,External data</td>         
                              <td>(OT)</td> </tr>
                           
                           <tr>
                              <td>#07</td> 
                              <td>:</td> 
                              <td>PMC error</td>                        
                              <td>(PC)</td> </tr>
                           
                           <tr>
                              <td>#08</td> 
                              <td>:</td> 
                              <td>External alarm message  (1)</td>      
                              <td>(EX)</td> </tr>
                           
                           <tr>
                              <td>#09</td> 
                              <td>:</td> 
                              <td>(Not used)</td>                       
                              <td></td>     </tr>
                           
                           <tr>
                              <td>#10</td> 
                              <td>:</td> 
                              <td>Serious P/S</td>                      
                              <td>(SR)</td> </tr>
                           
                           <tr>
                              <td>#11</td> 
                              <td>:</td> 
                              <td>(Not used)</td>                       
                              <td></td>     </tr>
                           
