                    "OT ALARM",     "OH ALARM",
                    "SERVO ALARM",  "SYSTEM ALARM",
                    "APC ALARM",    "SPINDLE ALARM",
                    "P/S ALARM (5000-)"
            };
            int alm;
            ushort idx;
            Focas1.cnc_alarm2(h, out alm);
            if (alm == 0)
            {
                    Console.WriteLine("NO ALARM");
            }
            else
            {
                    for (idx = 0; idx &lt; 11; idx++)
                    {
                            if ((alm &amp; 0x0001) &gt; 0)
